{
  "name": "Grafana2PDF",
  "version": "1.0",
  "description": "This extension will generate a PDF report of a Grafana dashboard",
  "browser_action": {
    "default_title": "Grafana2PDF",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "cookies",
    "storage",
    "*://*/*"
  ],
  "manifest_version": 2
}